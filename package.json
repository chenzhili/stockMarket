{
  "name": "QLStockMarket",
  "version": "1.0.0",
  "description": "这是一个有关 股市行情的 组件库",
  "keywords": [
    "股市",
    "曲线",
    "QL",
    "ql",
    "stockMarket"
  ],
  "private": false,
  "main": "core/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack-config/webpack-dev.js --color --progress",
    "start:vue": "cross-env NODE_ENV=development Frame=Vue webpack-dev-server --config ./webpack-config/webpack-dev.js --color --progress",
    "start:react": "cross-env NODE_ENV=development Frame=React webpack-dev-server --config ./webpack-config/webpack-dev.js --color --progress",
    "dev": "cross-env NODE_ENV=development webpack --config ./webpack-config/webpack-dev.js --color --progress",
    "dev:vue": "cross-env NODE_ENV=development Frame=Vue webpack --config ./webpack-config/webpack-dev.js --color --progress",
    "dev:react": "cross-env NODE_ENV=development Frame=React webpack --config ./webpack-config/webpack-dev.js --color --progress",
    "build:dll": "webpack --config ./webpack-config/webpack-dll.js --color --progress",
    "publish:vue:min": "cross-env MINIFY=true Frame=Vue webpack --progress --config ./webpack-config/webpack-bundle.js",
    "publish:vue:full": "cross-env MINIFY=false Frame=Vue webpack --progress --config ./webpack-config/webpack-bundle.js",
    "publish:vue": "npm run publish:vue:min && npm run publish:vue:full",
    "publish:react:min": "cross-env MINIFY=true Frame=React webpack --progress --config ./webpack-config/webpack-bundle.js",
    "publish:react:full": "cross-env MINIFY=false Frame=React webpack --progress --config ./webpack-config/webpack-bundle.js",
    "publish:react": "npm run publish:react:min && npm run publish:react:full",
    "publish:coms:vue": "cross-env Frame=Vue webpack --progress --config ./webpack-config/webapck-component.js",
    "publish:coms:react": "cross-env Frame=React webpack --progress --config ./webpack-config/webapck-component.js",
    "publish:common": "cross-env MINIFY=fals webpack --progress --config ./webpack-config/webpack-common.js",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "author": "ql",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/runtime": "^7.6.3",
    "@vue/test-utils": "^1.0.0-beta.32",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "glob-all": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.2.0",
    "jest-serializer-vue": "^2.0.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "purgecss-webpack-plugin": "^1.6.0",
    "react-test-renderer": "^16.13.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "vue": "^2.6.10"
  }
}
